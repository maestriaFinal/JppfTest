<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="JPPF Checks" location="/JPPF/CheckStyle_JPPF_config.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="JPPF Checks" local="true">
    <file-match-pattern match-pattern=".java" include-pattern="true"/>
  </fileset>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="TomcatPort/src/java"/>
    <filter-data value="&lt;recurse=false&gt;"/>
  </filter>
</fileset-config>
